{
  "name": "archivist",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "description": "Smart AI Image Library: Organize art, manage prompts, and archive assets with ease",
  "author": "SKB",
  "license": "MIT",
  "homepage": "https://github.com/skb/archivist",
  "build": {
    "appId": "com.archivist.app",
    "productName": "Archivist",
    "copyright": "Copyright Â© 2024-2026 SKB. All rights reserved.",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "dir",
      "icon": "public/icon.png"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:6173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "1.34.0",
    "chokidar": "3.6.0",
    "dexie": "3.2.4",
    "exiftool-vendored": "34.3.0",
    "lucide-react": "0.344.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3",
    "react-virtuoso": "4.7.1"
  },
  "devDependencies": {
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@vitejs/plugin-react": "5.1.2",
    "autoprefixer": "10.4.18",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "electron": "39.2.7",
    "electron-builder": "24.13.3",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.2.2",
    "vite": "7.3.0",
    "wait-on": "7.2.0"
  }
}